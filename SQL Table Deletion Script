SQL Table Deletion Script

-- These are the correct order not to mess up with the foreign key constraints.

DROP TABLE IF EXISTS FIGHT_ROUND_ASSISTANT;
DROP TABLE IF EXISTS AUDIENCE_FEEDBACK;
DROP TABLE IF EXISTS MEDICAL_REPORT;
DROP TABLE IF EXISTS OCTAGON_GIRL;
DROP TABLE IF EXISTS BET;
DROP TABLE IF EXISTS HOSTS;
DROP TABLE IF EXISTS FIGHT;
DROP TABLE IF EXISTS EVENT;
DROP TABLE IF EXISTS SPONSOR;
DROP TABLE IF EXISTS CAGE;
DROP TABLE IF EXISTS COMMENTATOR;
DROP TABLE IF EXISTS FIGHTER;
DROP TABLE IF EXISTS REFEREE;
